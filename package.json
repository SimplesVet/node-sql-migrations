{
  "name": "sql-migrations",
  "version": "1.0.5",
  "description": "raw SQL migrations library for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "mysql:test": "mocha ./test/mysql.spec.js",
    "mysql:migrate": "node ./migrate-mysql.js migrate",
