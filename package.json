    "type": "git",
    "url": "git@github.com:dmitriiabramov/node-sql-migrations.git"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "chalk": "^1.1.1",
    "mkdirp": "^0.5.0",
    "mocha": "^6.1.4",
    "pg": "^7.4.1",
    "promise-mysql": "^3.3.1"
  }
}
